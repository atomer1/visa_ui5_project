<mvc:View controllerName="visaproject.controller.DashboardPage"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core" displayBlock="true"
    xmlns:l="sap.ui.layout"
    xmlns="sap.m"
    xmlns:f="sap.ui.layout.form">
    <Page class="sapUiContentPadding">
        <headerContent>
            <core:Fragment fragmentName="visaproject.view.fragment.HeaderContent" type="XML" />
        </headerContent>
        <content>
            <!-- <core:Fragment fragmentName="visaproject.view.fragment.HeaderContent" type="XML" /> -->
            <core:Fragment class="sapUiTinyMarginTop" fragmentName="visaproject.view.fragment.SupplierPaymentMethod" type="XML" />
            <core:Fragment class="sapUiTinyMarginTop" fragmentName="visaproject.view.fragment.cardRegistering" type="XML" />
            <core:Fragment class="sapUiTinyMarginTop" fragmentName="visaproject.view.fragment.PaymentStatusTable" type="XML" />
        </content>
        <footer>
            <core:Fragment fragmentName="visaproject.view.fragment.FooterContent" type="XML" />
        </footer>
    </Page>
</mvc:View>
